<!DOCTYPE html>
<html>
<head>
  <title>funktion.fm</title>
  <meta charset="utf-8">
  <link href="{{static.root}}css/webfontkit/stylesheet.css" rel="stylesheet" type="text/css">
  <link href="{{static.root}}css/jquery.jscrollpane.css" rel="stylesheet" type="text/css" media="all" />
  <link href="{{static.root}}css/funktion.fm.css" rel="stylesheet" type="text/css">
  <script src="{{static.root}}js/funktion.fm.js"></script>
  <script src="http://platform.twitter.com/widgets.js"></script>
</head>

<body>
  <svg id="bgSvg"></svg>
  <div id="mainPage">

    <ul class="menu">
      <li class="contact"><a href="#/contact">funktion.fm</a></li>
      <li class="news"><a href="#/news">NEWS</a></li>
      <li class="projects"><a href="#/projects">PROJECTS</a></li>
      <li class="clearfix"></li>
    </ul>

    <div id="contactBody">
      <div class="title">funktion.fm</div>
      <div class="name">SÃ©bastien Piquemal</div>
      <div class="mail">sebpiq@gmail.com</div>
      <div class="social">
        <a href="https://twitter.com/sebpiq"><img src="images/twitter.png" /></a>
        <a href="https://github.com/sebpiq"><img src="images/github.png" /></a>
        <a href="https://soundcloud.com/sebpiq"><img src="images/soundcloud.png" /></a>
        <a href="https://vimeo.com/sebpiq"><img src="images/vimeo.png" /></a>
      </div>
    </div>

    <div id="newsBody">
      {{#each getPosts}}
      <div class="summary">
        <div class="title">
          <div class="date">{{prettifyDate date}}</div>
          <a href="#post/{{slug}}">{{title}}</a>
        </div>
        <div class="preview">{{{preview}}}</div>
        <div class="clearfix"></div>
      </div>
      {{/each}}
    </div>

    <div id="postBody"></div>

    <div id="projectsBody">

      <div class="project tile1" data-href="#projects/fields">
        <div class="thumbnail">
          <img src="images/projects/fields-video-1.jpg" style="margin-top:65px" />
        </div>
        <div class="content">
          <iframe src="//player.vimeo.com/video/92586493?title=0&amp;byline=0&amp;portrait=0" width="300" height="169" frameborder="0" webkitallowfullscreen mozallowfullscreen allowfullscreen style="margin-top:65px"></iframe>
        </div>
      </div>

      <div class="project tile1" data-href="#projects/pd-rpi-workshop">
        <div class="thumbnail">
          <img src="images/projects/pd-rpi-workshop-1.jpg" style="width:100%" />
        </div>
        <div class="content">
          <iframe src="//player.vimeo.com/video/76307579?title=0&amp;byline=0&amp;portrait=0" width="300" height="169" frameborder="0" webkitallowfullscreen mozallowfullscreen allowfullscreen></iframe>
        </div>
      </div>

      <div class="project tile3" data-href="#projects/pd-rpi-workshop">
        <div class="thumbnail">
          <img src="images/projects/pd-rpi-workshop-1.jpg" style="width:100%" />
        </div>
        <div class="content">
          <img src="images/projects/pd-rpi-workshop-1.jpg" style="width:100%" />
        </div>
      </div>

      <div class="project tile3" data-href="#projects/pd-rpi-workshop">
        <div class="thumbnail">
          <img src="images/projects/pd-rpi-workshop-2.jpg" style="width:100%" />
        </div>
        <div class="content">
          <img src="images/projects/pd-rpi-workshop-2.jpg" style="width:100%" />
        </div>
      </div>

      <div class="project tile3" data-href="#projects/pd-rpi-workshop">
        <div class="thumbnail">
          <img src="images/projects/pd-rpi-workshop-3.jpg" style="width:100%" />
        </div>
        <div class="content">
          <img src="images/projects/pd-rpi-workshop-3.jpg" style="width:100%" />
        </div>
      </div>

      <div class="project tile3" data-href="#projects/pd-rpi-workshop">
        <div class="thumbnail">
          <img src="images/projects/pd-rpi-workshop-4.jpg" style="width:100%" />
        </div>
        <div class="content">
          <img src="images/projects/pd-rpi-workshop-4.jpg" style="width:100%" />
        </div>
      </div>


      {{#each projectPosts}}
      <div class="project tile2" data-href="#projects/{{project}}">
        <div class="thumbnail">
          <div class="title">{{title}}</div>
        </div>
        <div class="content">
          {{{preview}}}
        </div>        
      </div>
      {{/each}}

    </div>

  </div>

  <script>
    (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
    (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
    m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
    })(window,document,'script','//www.google-analytics.com/analytics.js','ga');

    ga('create', 'UA-17637518-4', 'funktion.fm');
    ga('send', 'pageview');
  </script>

</body>

</html>
